meme_dict = {
            "CRINGE": "Garip ya da utandırıcı bir şey",
            "LOL": "Komik bir şeye verilen cevap",
            "BRUH":"Olup olmadık herşeye verilen anlamsız kelime",
            "ASAP":"En yakın zamanda",
            "NGL":"Yalan söylemeyeceğim",
            "NT":"İyi denemeydi",
            "GG":"İyi oyundu",
            "GJ":"İyi iş",
            "RN":"Şimdi",
            "WP":"İyi oyundu"
            }
word = input("Anlamadığınız bir kelime yazın (hepsini büyük harflerle yazın!): ").upper()
if word in meme_dict.keys():
    print(meme_dict[word])
else:
    print("Bu kelime sözükte yok")
